#include <bits/stdc++.h>

using namespace std;

int main() {
    // your code goes here
    int t;
    cin >> t;
    while (t--) {
        int sleephours;
        cin >> sleephours;
        if (sleephours < 7)
            std::cout << "YES" << std::endl;
        else
            std::cout << "NO" << std::endl;
    }

}
