#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int X,Y;
	    cin>>X>>Y;
	    if(2*X>5*Y)
	    std::cout << "CHOCOLATE" << std::endl;
	    else if(2*X<5*Y)
	    std::cout << "CANDY" << std::endl;
	    else
	    std::cout << "EITHER" << std::endl;
	  }
}
